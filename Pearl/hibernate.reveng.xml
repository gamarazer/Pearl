<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="photo" catalog="pearl"
		class="cn.jk.pearl.pojo.Photo">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="news" catalog="pearl" class="cn.jk.pearl.pojo.News">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="datacollection" catalog="pearl"
	class="cn.jk.pearl.pojo.DataCollection">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>